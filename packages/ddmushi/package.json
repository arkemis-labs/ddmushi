{
    "name": "ddmushi",
    "version": "0.2.0",
    "description": "A tRPC-like API organization library for TanStack React Query",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "check-types": "tsc --noEmit"
    },
    "keywords": [
        "api",
        "react-query",
        "tanstack"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@tanstack/react-query": "^5.0.0"
    },
    "devDependencies": {
        "@ddmushi/typescript-config": "workspace:*",
        "@types/react": "^18.0.0",
        "@vitest/coverage-v8": "^2.0.0",
        "@vitest/ui": "^2.0.0",
        "jsdom": "^24.0.0",
        "tsup": "^8.0.0",
        "typescript": "5.8.3",
        "vitest": "^2.0.0"
    },
    "peerDependencies": {
        "@tanstack/react-query": ">=5.0.0",
        "react": ">=18.0.0"
    }
}